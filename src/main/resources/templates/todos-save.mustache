{{>layout/header}}
<h1>save todos</h1>

<div class="col-md-12">
    <div class="col-md-4">
        <form>
            <div class="form-group">
                <label for="todo">Todo</label>
                <input type="text" class="form-control" id="todo" placeholder="input todo" />
            </div>
            <div class="form-group">
                <label for="memo">Memo</label>
                <input type="text" class="form-control" id="memo" placeholder="input memo"/>
            </div>
        </form>
        <a href="/" role="button" class="btn btn-secondary">cancel</a>
        <button type="button" class="btn btn-primary" id="btn-save">save</button>
    </div>
</div>
{{>layout/footer}}